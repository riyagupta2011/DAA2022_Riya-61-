//ques1

#include<iostream>
using namespace std;

int main()
{
    int n,i,k,c=0;
    cin>>n;
    int a[n];
    cout<<"enter array elements"<<endl;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cout<<"enter the value of key"<<endl;
    cin>>k;
    for(i=0;i<n;i++)
    {
       if(a[i]==k)
       {
           cout<<"key is present at "<<i<<" index"<<endl;
           break;
       }
       c++;
    }
    if(c>n-1)
    {
        cout<<"key not found"<<endl;
    }
    cout<<"no of comaprisions are"<<c;

}
