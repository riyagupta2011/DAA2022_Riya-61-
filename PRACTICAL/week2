//ques1

#include <iostream>
using namespace std;
int main()
{
    int n,flag=0,c=0,i,k;
    cin>>n;
    int a[n];
    cout<<"enter number in arrays"<<endl;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cout<<"enter value of key"<<endl;
    cin>>k;
    for(i=0;i<n;i++)
    {
        if(a[i]==k)
        {
            c++;
            flag=1;
        }

    }
    if(flag==0)
    {
        cout<<"key not present -1";

    }
    else
        cout<<"key is present "<<c;
