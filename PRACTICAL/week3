#include<iostream>
using namespace std;

int main()
{
    int i,j,n,c=0;
   cout<<"enter no of element in arrays"<<endl;
   cin>>n;
   cout<<"enter elements in an array"<<endl;
   int a[n];
   for(i=0;i<n;i++)
   {
       cin>>a[i];
   }
    for(i=1;i<n;i++)
   {
       int k=a[i];
       j=i-1;
       
       while(k<a[j]&&j>=0)
       {
           a[j+1]=a[j];
           j--;
           c++;
       }
       a[j+1]=k;

   }
   for(i=0;i<n;i++)
   {
       cout<<a[i]<<" ";
   }
     cout<<"\n"<<"number of comparision are "<<c;


}
